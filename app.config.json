{
  "environmentUrl": "https://cer89933.apps.dynatrace.com/",
  "app": {
    "name": "KPI-dashboard",
    "version": "0.0.0",
    "description": "A starting project with routing, fetching data, and charting",
    "id": "my.kpi.dashboard",
    "scopes": [
      { "name": "storage:logs:read", "comment": "default template" },
      { "name": "storage:buckets:read", "comment": "default template" },
      { "name": "environment-api:problems:read", "comment": "problem scope" },
      { "name": "storage:events:read", "comment": "event scope" },
      { "name": "storage:buckets:read", "comment": "bucket scope" },
      { "name": "storage:metrics:read", "comment": "storage scope" },
      {
        "name": "state:app-states:write",
        "comment": "Access to write app state"
      },
      {
        "name": "state:app-states:read",
        "comment": "Access to read app state"
      },
      {
        "name": "state:app-states:delete",
        "comment": "Acess for deleting app states"
      },
      {
        "name": "state:user-app-states:read",
        "comment": "Read Access to user app state"
      },
      {
        "name": "state:user-app-states:write",
        "comment": "Write Access to user app state"
      },
      {
        "name": "state:user-app-states:delete",
        "comment": "Acess for deleting user app states"
      },
      {
        "name": "document:documents:write",
        "comment": " Document to write app data"
      },
      {
        "name": "document:documents:read",
        "comment": " Access to read document data"
      }
    ]
  }
}
